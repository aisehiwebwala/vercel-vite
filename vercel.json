{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static_build"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
